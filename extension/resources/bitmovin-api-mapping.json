{
  "version": "0.1",
  "pages": [
    {
      "description": "Any page",
      "url": "/encoding",
      "mappings": [
        {
          "description": "Highlight of identifier",
          "type": "highlight",
          "jsonpath": "$.data.result.id",
          "presentation": {
            "highlight": "identifier"
          }
        },
        {
          "description": "Highlight of error",
          "type": "highlight",
          "jsonpath": "$.data.details[?(@.type=='ERROR')].*",
          "presentation": {
            "highlight": "error"
          }
        },
        {
          "description": "Highlight of error",
          "type": "highlight",
          "jsonpath": "$.data.developerMessage",
          "presentation": {
            "highlight": "error"
          }
        },
        {
          "description": "Highlight of error",
          "type": "highlight",
          "jsonpath": "$.data.message",
          "presentation": {
            "highlight": "error"
          }
        },
        {
          "description": "Highlight of error",
          "type": "highlight",
          "jsonpath": "$..*[?(@.status=='ERROR')].status",
          "presentation": {
            "highlight": "error"
          }
        },

        {
          "description": "Link to input objects",
          "type": "href",
          "jsonpath": "$..inputId",
          "variables": [
            {
              "name": "type",
              "type": "xhr",
              "url": "/encoding/inputs/{value}/type",
              "jsonpath": "$.data.result.type",
              "transformation": "lowerCase"
            }
          ],
          "target": {
            "url": "/encoding/inputs/{type}/{value}"
          }
        },
        {
          "description": "Link to output objects",
          "type": "href",
          "jsonpath": "$..outputId",
          "variables": [
            {
              "name": "type",
              "type": "xhr",
              "url": "/encoding/outputs/{value}/type",
              "jsonpath": "$.data.result.type",
              "transformation": "lowerCase"
            }
          ],
          "target": {
            "url": "/encoding/outputs/{type}/{value}"
          }
        }
      ]
    },
